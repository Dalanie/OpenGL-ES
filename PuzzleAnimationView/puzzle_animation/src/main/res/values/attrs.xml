<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PuzzleImageView">
        <attr name="hide_background" format="integer" />
        <attr name="cube_rows" format="integer" />
        <attr name="cube_columns" format="integer" />
        <attr name="puzzle_mode" format="integer">
            <enum name="click" value="0" />
            <enum name="click_random" value="1" />
            <enum name="time" value="2" />
        </attr>
        <attr name="milliseconds_to_release" format="integer" />
    </declare-styleable>
</resources>